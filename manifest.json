{
    "manifest_version": 2,
    "name": "SET Solver",
    "version": "1.0",
    "author": "Tal Dromi",
    "description": "Instantaneously solving the New York Times SET puzzle",
    "icons": {
        "48": "icon.png"
    },
    "content_scripts": [
        {
            "run_at" :"document_end",
            "matches": [
                "https://www.nytimes.com/puzzles/set"
            ],
            "css": [
                "style.css"
            ],
            "js": [
                "solver.js"
            ]
        }
    ]
}
